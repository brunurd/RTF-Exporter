<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <AssemblyName>RTFExporter</AssemblyName>
        <RootNamespace>RTFExporter</RootNamespace>
        <PackageId>RTFExporter</PackageId>
        <Title>RTFExporter</Title>
        <Version>1.1.4</Version>
        <Authors>Bruno Araujo</Authors>
        <Company>Lava Leak</Company>
        <Copyright>Copyright (c) 2017-2020 Bruno Araujo</Copyright>
        <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/brunurd/RTFExporter</PackageProjectUrl>
        <RepositoryUrl>https://github.com/brunurd/RTFExporter</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Description>A C# library to generate .RTF text files from any string object data, stylized and ready for any text processor. No fancy dependencies or restrictive licenses.</Description>
        <PackageReleaseNotes>chore: Copyright update.</PackageReleaseNotes>
        <PackageTags>rtf;rich;text;doc;docx;word;string</PackageTags>
        <LangVersion>4</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\LICENSE.md" Pack="true" PackagePath=""/>
        <None Include="..\README.md" Pack="true" PackagePath=""/>
    </ItemGroup>
</Project>
